                <Control width="57"/>
                <Button width="68" name="manager_patient_detail_return" normalimage="btn_return.png" hotimage="btn_return.png" pushedimage="btn_return.png"/>
                <Control />
            </HorizontalLayout>
            <Control />
        </VerticalLayout>
        <VerticalLayout width="1203">
            <Control height="59"/>
